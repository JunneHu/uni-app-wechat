<view><view class="calendar"><view class="c-top"><view data-event-opts="{{[['tap',[['changeDate',['MU']]]]]}}" class="item pre" onTap="__e">上月</view><view class="item txt">{{monthTxt}}</view><view data-event-opts="{{[['tap',[['changeDate',['MD']]]]]}}" class="item next" onTap="__e">下月</view></view><view class="head"><view class="rows"><text>日</text><text>一</text><text>二</text><text>三</text><text>四</text><text>五</text><text>六</text></view></view><swiper class="swiper" style="{{('height:'+sheight+'px')}}" current="{{current}}" circular="true" data-event-opts="{{[['change',[['change',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{swiper}}" a:for-item="sitem" a:for-index="sindex" a:key="sindex"><view class="week" a:for="{{normalDateList}}" a:for-item="index" a:for-index="i" a:key="i"><view class="rows tr1"><block a:for="{{normalDateList[i]}}" a:for-item="noritem" a:for-index="idx" a:key="idx"><text data-event-opts="{{[['tap',[['choose',['$0',i],[[['normalDateList.'+i+'','',idx,'text']]]]]]]}}" class="{{(((idx==0||idx==6)?'weekend':'')+' '+((noritem.isToday)?'today':''))}}" onTap="__e"><text class="txt">{{noritem.text}}</text></text></block></view><view class="rows tr2"><block a:for="{{lunarDateList[i]}}" a:for-item="lunitem" a:for-index="idx" a:key="idx"><text data-event-opts="{{[['tap',[['choose',['$0',i],[[['lunarDateList.'+i+'','',idx,'days']]]]]]]}}" class="{{(((idx==0||idx==6)?'weekend':'')+' '+((lunitem.isToday)?'today':''))}}" onTap="__e">{{lunitem.text}}</text></block></view></view></swiper-item></swiper></view></view>