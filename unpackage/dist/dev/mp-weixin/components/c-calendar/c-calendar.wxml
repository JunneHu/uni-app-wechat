<view><view class="calendar"><view class="c-top"><view data-event-opts="{{[['tap',[['changeDate',['MU']]]]]}}" class="item pre" bindtap="__e">上月</view><view class="item txt">{{monthTxt}}</view><view data-event-opts="{{[['tap',[['changeDate',['MD']]]]]}}" class="item next" bindtap="__e">下月</view></view><view class="head"><view class="rows"><text>日</text><text>一</text><text>二</text><text>三</text><text>四</text><text>五</text><text>六</text></view></view><swiper class="swiper" style="{{('height:'+sheight+'px')}}" current="{{current}}" circular="true" data-event-opts="{{[['change',[['change',['$event']]]]]}}" bindchange="__e"><block wx:for="{{swiper}}" wx:for-item="sitem" wx:for-index="sindex" wx:key="sindex"><swiper-item><block wx:for="{{normalDateList}}" wx:for-item="index" wx:for-index="i" wx:key="i"><view class="week"><view class="rows tr1"><block wx:for="{{normalDateList[i]}}" wx:for-item="noritem" wx:for-index="idx" wx:key="idx"><block><text data-event-opts="{{[['tap',[['choose',['$0',i],[[['normalDateList.'+i+'','',idx,'text']]]]]]]}}" class="{{[(idx==0||idx==6)?'weekend':'',(noritem.isToday)?'today':'']}}" bindtap="__e"><text class="txt">{{noritem.text}}</text></text></block></block></view><view class="rows tr2"><block wx:for="{{lunarDateList[i]}}" wx:for-item="lunitem" wx:for-index="idx" wx:key="idx"><block><text data-event-opts="{{[['tap',[['choose',['$0',i],[[['lunarDateList.'+i+'','',idx,'days']]]]]]]}}" class="{{[(idx==0||idx==6)?'weekend':'',(lunitem.isToday)?'today':'']}}" bindtap="__e">{{lunitem.text}}</text></block></block></view></view></block></swiper-item></block></swiper></view></view>