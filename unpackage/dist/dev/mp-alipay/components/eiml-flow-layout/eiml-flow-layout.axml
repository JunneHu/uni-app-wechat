<view class="viewSpaceBetween"><view class="column" style="{{'width:'+(columnWidth+'%')+';'}}" a:for="{{tabList}}" a:for-item="itemTab" a:for-index="indexTab" a:key="indexTab"><view a:for="{{itemTab}}" a:for-item="item" a:for-index="index" a:key="index"><view style="padding:10rpx 5rpx 0rpx 5rpx;"><view style="padding:10rpx;border-radius:10rpx;background:white;"><image style="width:100%;" src="{{item.logo}}" mode="widthFix"></image><view class="itemName">{{''+item.name+''}}</view><block a:if="{{item.director}}"><view class="itemTitle">{{''+item.director+''}}</view></block><block a:if="{{item.actors}}"><view class="itemPrice">{{''+item.actors+''}}</view></block></view></view></view></view><scroll-view style="width:0rpx;height:0rpx;"><view id="{{'child'+index}}" a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view style="padding:10rpx 5rpx 0rpx 5rpx;"><view style="padding:10rpx;border-radius:10rpx;background:white;"><image style="width:100%;" src="{{item.logo}}" mode="widthFix"></image><view class="itemName">{{''+item.name+''}}</view><block a:if="{{item.director}}"><view class="itemTitle">{{''+item.director+''}}</view></block><block a:if="{{item.actors}}"><view class="itemPrice">{{''+item.actors+''}}</view></block></view></view></view></scroll-view></view>